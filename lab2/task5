#include<stdio.h>
#include<math.h>
int main(){
double x,y;
printf("enter x and y\n");
scanf("%lf %lf",&x,&y);
if ((pow(x,2) + pow(y,2) < 1)&& y>=0 && x>=0 && (pow(x,2)+ pow(y,2)>pow(0.5,2))) printf("yes\n");
else printf("no\n");
}
